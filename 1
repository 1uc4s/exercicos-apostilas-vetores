vetor1 = []
vetor2 = []
soma = []

for i in range(3):
    vetor1.append(int(input('Insira o valor do primeiro vetor(na ordem x,y,z): ')))
for i in range(3):
    vetor2.append(int(input('Insira o valor do segundo vetor(na ordem x,y,z): ')))

for i in range(3):
    soma.append(vetor1[i] + vetor2[i])

print("vetor 1", vetor1)
print("vetor 2", vetor2)
print("soma dos vetores", soma)
